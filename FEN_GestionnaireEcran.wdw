     type_code : 14
     code : |1+
      // Résumé : Ajoute une ligne de texte dans le champ texte SAI_Stat
      // Syntaxe :
      //AjouteLigneDansStatus (<sLigneAAjouter> est chaîne)
      //
      // Paramètres :
      //	sLigneAAjouter (chaîne UNICODE) : Texte à ajouter au status de l'application
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // AjouteLigneDansStatus("Tout va très bien, madame la marquise.")
      //
      PROCEDURE AjouteLigneDansStatus(sLigneAAjouter est une chaîne)
      ExécuteThreadPrincipal(AjouteLigneDansStatusT,sLigneAAjouter)
      
     type : 458752
   -
     name : AjouteLigneDansStatusT
     procedure_id : 2290287011540495863
     type_code : 14
     code : |1+
      // Résumé : Procédure exécutée dans le Thread principal depuis AjouteLigneDansStatus
      // Syntaxe :
      //AjouteLigneDansStatusT (<sLigneAAjouter> est chaîne)
      //
      // Paramètres :
      //	sLigneAAjouter (chaîne UNICODE) : Chaine de texte à rajouter
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCÉDURE AjouteLigneDansStatusT(sLigneAAjouter est une chaîne)
      gsStatusTexte += sLigneAAjouter + RC
      SAI_Stat = gsStatusTexte
      
      
     type : 458752
   -
     name : CB_Erreur
     procedure_id : 2290287011540561399
     type_code : 14
     code : |1-
      // Résumé : Procédure exécutée lorsqu'une méthode PrintConnect renvoie une erreur
      // Syntaxe :
      //CB_Erreur (<sProfilMx> est chaîne, <sMessage> est chaîne, <sErreur> est chaîne)
      //
      // Paramètres :
      //	sProfilMx (chaîne UNICODE) : Le profil MX concerné par l'erreur
      //	sMessage (chaîne UNICODE) : Le message associé à l'erreur
      //	sErreur (chaîne UNICODE) : L'erreur qui s'est produite au format JSON
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // CB_Erreur("Une erreur s'est produite lors de la récupération du Status","PC_NO_PRINTER_SELECTED")
      //
      PROCÉDURE CB_Erreur(sProfilMx est une chaine, sMessage est une chaine, sErreur est une chaîne)
      AjouteLigneDansStatus("-------------------------------------------")
      AjouteLigneDansStatus(sMessage)
      AjouteLigneDansStatus("-------------------------------------------")
      SI sErreur <> "" ALORS
      	AjouteLigneDansStatus("Détail de l'erreur: ")
      	AjouteLigneDansStatus("-------------------------------------------")
      	AjouteLigneDansStatus(sErreur)
      	AjouteLigneDansStatus("-------------------------------------------")
      FIN
      AjouteLigneDansStatus("Profil MX concerné: ")
      AjouteLigneDansStatus("-------------------------------------------")
      AjouteLigneDansStatus(sProfilMx)
