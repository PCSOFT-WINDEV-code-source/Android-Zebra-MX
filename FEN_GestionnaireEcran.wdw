  -
    name : BTN_Executer
    identifier : 0x2fc8bbe72703edf7
    internal_properties : 
    properties :
     x : 90
     y : 312
     width : 140
     height : 31
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        internal_properties : 
        code : |1-
         // Dans cet exemple, nous allons réaliser une fenêtre qui nous permettra de contrôler dynamiquement
         // par programmation la durée avant la mise en veille de l'écran.
         
         // Pour cela, nous allons directement récupérer un fichier XML exporté depuis l'outil StageNow
         // et exécuter cette chaine XML depuis Windev Mobile
         
         // Utilitaire StageNow
         // https://www.zebra.com/fr/fr/products/software/mobile-computers/mobile-app-utilities/stagenow.html
         
         // Documentation de StageNow
         // https://techdocs.zebra.com/stagenow/4-2/about/
         
         // Documentation du DisplayMgr
         // https://techdocs.zebra.com/stagenow/4-2/csp/display/
         
         // Exporter un profil StageNow vers un MDM (création d'un fichier XML contenant le profil StageNow)
         // https://techdocs.zebra.com/stagenow/4-2/stagingprofiles/#exportingstagingprofiles
         
         // Voici la chaine XML résultant de la création de notre profil StageNow après export vers un MDM
         // pour un délai avant mise en veille de l'écran de 10 min = 600 sec
         
         // <wap-provisioningdoc>
         //   <characteristic version="4.3" type="DisplayMgr">
         //     <parm name="TimeoutInterval" value="600" />
         //   </characteristic>
         // </wap-provisioningdoc>
         
         // Afin de rendre ce profil dynamique, nous allons remplacer la valeur "600" par "%TIMEOUT%"
         
         // La chaine devient:
         // <wap-provisioningdoc>
         //   <characteristic version="4.3" type="DisplayMgr">
         //     <parm name="TimeoutInterval" value="%TIMEOUT%" />
         //   </characteristic>
         // </wap-provisioningdoc>
        type : 14
      -
        internal_properties : CAAAAAgAAACP1zVAqZ+dmobYDQNPpMCM1DnzlZg8GTHGtJv9mL3BJqkHlNwb51iY5hwTrA5oHoRZnvu+w84fuS4x90H4dOh7Ut7Jhmgr/Wyc3s6xx6DjpWCpmqXAzvrrAI6VReQ3cMKrtfvdUWACCUbeJwwECcD6pp3EjawfXhdXFWhvsbIjtgNDgl7alxa3rzP9Ha8IA17OXLygxFDPER/+hxFQ24dzSPR6oehcNs/npdhwlA==
        code : |1+
         gsStatusTexte = "Exécution du profil en cours" + RC
         SAI_Stat = gsStatusTexte
         
         // On copie colle le fichier XML généré par StageNow
         sChaineXMLaExecuter est une chaine = [
         	<wap-provisioningdoc>
         		<characteristic version="4.3" type="DisplayMgr">
         			<parm name="TimeoutInterval" value="%TIMEOUT%" />
         		</characteristic>
         	</wap-provisioningdoc>
         ]
         
         SI SEL_ScreenTimeoutInterval = 1 ALORS
         	// On a sélectionné "ne rien changer"
         	// Alors on ne fait rien ;)
         	sChaineXMLaExecuter = Remplace(sChaineXMLaExecuter,"%TIMEOUT%", "Do not change")
         	CB_Succes(sChaineXMLaExecuter)
         	RETOUR
         SINON
         	// Suivant l'option sélectionnée par l'utilisateur, nous allons attribuer
         	// la durée avant mise en veille de l'écran du terminal
         	nDuréeAvantMiseEnVeille est un entier
         	
         	SELON SEL_ScreenTimeoutInterval
         		// 15 sec
         		CAS 2
         			nDuréeAvantMiseEnVeille = 15
         		// 30 sec
         		CAS 3
         			nDuréeAvantMiseEnVeille = 30
         		// 1 min
         		CAS 4
         			nDuréeAvantMiseEnVeille = 60
         		// 2 min
         		CAS 5
         			nDuréeAvantMiseEnVeille = 120
         		// 5 min
         		CAS 6
         			nDuréeAvantMiseEnVeille = 300
         		// 10 min
         		CAS 7
         			nDuréeAvantMiseEnVeille = 600
         		// 30 min
         		CAS 8
         			nDuréeAvantMiseEnVeille = 1800
         	FIN
         	
         	sChaineXMLaExecuter = Remplace(sChaineXMLaExecuter,"%TIMEOUT%", nDuréeAvantMiseEnVeille)
         	ZebraMXExecuterLeProfil("",sChaineXMLaExecuter,"FEN_GestionnaireEcran.CB_Succes","FEN_GestionnaireEcran.CB_Erreur")
         
         FIN
         
        type : 18
      -
        type : 161
        enabled : false
